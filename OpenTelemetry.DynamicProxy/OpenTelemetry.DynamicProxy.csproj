<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <DebugSymbols>True</DebugSymbols>
    <Description>Automatically generate activity by declare [Activity].</Description>
    <GeneratePackageOnBuild Condition="'$(Configuration)' == 'Release'">True</GeneratePackageOnBuild>
    <IncludeSymbols>True</IncludeSymbols>
    <RepositoryUrl>https://github.com/pengweiqhca/opentelemetry-proxy</RepositoryUrl>
    <PackageId>PW.OpenTelemetry.DynamicProxy</PackageId>
    <PackageProjectUrl>https://github.com/pengweiqhca/opentelemetry-proxy/tree/main/OpenTelemetry.DynamicProxy</PackageProjectUrl>
    <PackageTags>apm track profiling tracer otlp OpenTelemetry proxy Intercept</PackageTags>
    <TargetFrameworks>net461;netstandard2.0;netstandard2.1</TargetFrameworks>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>OpenTelemetry.DynamicProxy.Tests, </_Parameter1>
    </AssemblyAttribute>
    <PackageReference Include="Castle.Core" Version="4.4.1" />

    <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="6.0.0" Condition="'$(TargetFramework)' != 'netstandard2.1'" />
    <PackageReference Include="Microsoft.SourceLink.GitLab" Version="1.*" PrivateAssets="All" />
    <PackageReference Include="OpenTelemetry.Api" Version="1.1.0" />
  </ItemGroup>

</Project>
